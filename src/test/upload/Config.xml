	<!-- *************************************************** -->
	<!-- These are the interfaces that we can post transaction to JTWin with. 
		We can also define human (email) interfaces. Conditions can be placed on 
		interfaces, but we can still only have one interface per type. Conditions 
		must return nodes. We will use the interface if we can find > 0 matching 
		nodes in the document. -->
	<!-- *************************************************** -->

	<interfaces>
		<!-- <interface type="810" condition="/transaction[count(descendant::ASLPartnerName[text() 
			= 'Maax' or text() = 'Masco']) != 0]"> <email>dave@myprivateserver.com</email> 
			<emailFormat>pdf</emailFormat> <DSArchive>invoice</DSArchive> </interface> 
			<interface type="810" condition="/transaction[count(descendant::ASLPartnerName[text() 
			= 'Maax' or text() = 'Masco']) = 0]"> <email>david.manning@sheret.com</email> 
			<emailFormat>pdf</emailFormat> <DSArchive>invoice</DSArchive> </interface> -->
		<interface type="810">
			<email>david.manning@sheret.com</email>
			<emailFormat>pdf</emailFormat>
			<!-- emailFrom>810EDI@sheret.com</emailFrom -->
			<DSArchive>invoice</DSArchive>
		</interface>

		<interface type="810" condition="EXCEPTION">
			<email>dave@myprivateserver.com</email>
			<emailFormat>pdf</emailFormat>
		</interface>

		<!-- interface type="810"> <URL>http://10.99.225.100:8080/BBXBridge/BBXBRunner?module=EDIInvoiceSubmit 
			</URL> <URLFormat>xini</URLFormat> </interface -->

		<!-- Only send failed 997s to me. -->
		<interface type="997" condition="//status[text()='rejected']">
			<URL />
			<email>david.manning@sheret.com</email>
			<emailFormat>pdf</emailFormat>
		</interface>
	</interfaces>

