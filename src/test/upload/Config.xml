	<!-- *************************************************** -->
	<!-- This config file is really just for development/testing. Every installed 
		copy has it's own that we use in that environment. -->
	<!-- *************************************************** -->

	<!-- The base URL for calling our application. -->
	<baseURL>http://localhost:8080/EDIInterface</baseURL>

	<!-- The type of ID we are using: 01 - DUNS number 12 - Phone Number 13 
		- UCS Code or Other ZZ - Mutually defined -->

	<IDQualifier>12</IDQualifier>

	<!-- Our EDI ID. -->
	<ID>2503867FTP</ID>

	<!-- Our business name -->
	<Name>ANDREW SHERET LTD.</Name>

	<!-- Who do we email if we can't deliver? -->
	<administrator>david.manning@sheret.com</administrator>

	<!-- *************************************************** -->
	<!-- Defines the PAM users and groups who are allowed to use this app. -->
	<!-- *************************************************** -->
	<authentication>
		<provider>com.sheret.utils.ASLUPAMAuth</provider>
		<PAMService>sshd</PAMService>

		<allowUser>sysadmin</allowUser>
		<allowUser>dave</allowUser>

		<!-- allowGroup>adm</allowGroup -->

	</authentication>

